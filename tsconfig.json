{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@page_component/*": ["./lib/components/pages/*"],
      "@ui/*": ["./lib/components/ui/*"],
      "@types/*": ["./lib/utils/types/*"],
      "@scripts/*": ["./lib/utils/scripts/*"],
      "@app/*": ["./app/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "lib/components/hooks/darkModeContext.js",
    "lib/components/hooks/darkModeContext.js",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
